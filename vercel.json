{
  "crons": [
    {
      "path": "/api/cleanUp",
      "schedule": "0 8 * * *"
    }
  ],
  "functions": {
    "api/get.js": {
      "memory": 1024
    },
    "api/post.ts": {
      "memory": 1024
    },
    "api/cleanUp.ts": {
      "memory": 1024
    }
  },
  "routes": [
    { "src": "/api/get", "methods": ["GET"], "dest": "/api/get.js" },
    { "src": "/api/post", "methods": ["POST"], "dest": "/api/post.ts" },
    { "src": "/api/cleanUp", "status": 404, "continue": false}
  ]
}