{
  "crons": [
    {
      "path": "/api/cleanUp",
      "schedule": "0 8 * * *"
    }
  ],
  "routes": [
    { "src": "/api/get", "methods": ["GET"], "dest": "/api/get.js" },
    { "src": "/api/post", "methods": ["POST"], "dest": "/api/post.js" },
    { "src": "/api/cleanUp", "status": 404, "continue": false}
  ]
}