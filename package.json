{
  "name": "analog",
  "version": "0.1.2",
  "description": "A minimal analytics tool to self-host.",
  "main": "index.js",
  "scripts": {
    "build": "vite build",
    "dev": "concurrently 'vite' 'DEBUG=ioredis:* npx tsx watch --env-file=.env.local src/services/server/index.ts'",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "RCKT",
  "license": "MIT",
  "type": "module",
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@types/node": "^24.9.1",
    "@types/pg": "^8.15.5",
    "concurrently": "^9.2.1",
    "eslint": "^9.38.0",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "typescript-eslint": "^8.46.2",
    "vite": "^7.1.11"
  },
  "dependencies": {
    "@libsql/client": "^0.15.15",
    "@netlify/functions": "^5.0.1",
    "@netlify/vite-plugin": "^2.7.5",
    "@vercel/node": "^5.4.1",
    "ioredis": "^5.8.2",
    "mongodb": "^6.20.0",
    "pg": "^8.16.3",
    "toad-scheduler": "^3.1.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "uuid": "^13.0.0"
  },
  "engines": {
    "node": "22.x"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,mjs}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{html,css}": [
      "prettier --write",
      "git add"
    ]
  }
}
