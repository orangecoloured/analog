{
  "name": "analog",
  "version": "0.0.1",
  "description": "A minimal analytics tool to self-host.",
  "main": "index.js",
  "scripts": {
    "build": "vite build",
    "dev": "concurrently 'vite' 'DEBUG=ioredis:* npx tsx watch --env-file=.env.local src/services/server/index.ts'",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "RCKT",
  "license": "MIT",
  "type": "module",
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@types/node": "^24.0.1",
    "concurrently": "^9.1.2",
    "eslint": "^9.28.0",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.0",
    "prettier": "^3.5.3",
    "typescript-eslint": "^8.34.0",
    "vite": "^6.3.5"
  },
  "dependencies": {
    "@netlify/functions": "^4.1.5",
    "@netlify/vite-plugin": "^2.2.3",
    "@vercel/node": "^5.2.2",
    "ioredis": "^5.6.1",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "uuid": "^11.1.0"
  },
  "engines": {
    "node": "22.x"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,mjs}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{html,css}": [
      "prettier --write",
      "git add"
    ]
  }
}
