{
  "name": "analog",
  "version": "0.1.2",
  "description": "A minimal analytics tool to self-host.",
  "main": "index.js",
  "scripts": {
    "build": "vite build",
    "dev": "concurrently 'vite' 'DEBUG=ioredis:* npx tsx watch --env-file=.env.local src/services/server/index.ts'",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "RCKT",
  "license": "MIT",
  "type": "module",
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@types/node": "^24.1.0",
    "@types/pg": "^8.15.5",
    "concurrently": "^9.2.0",
    "eslint": "^9.32.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "typescript-eslint": "^8.38.0",
    "vite": "^7.0.6"
  },
  "dependencies": {
    "@libsql/client": "^0.15.10",
    "@netlify/functions": "^4.2.1",
    "@netlify/vite-plugin": "^2.5.4",
    "@vercel/node": "^5.3.10",
    "ioredis": "^5.7.0",
    "mongodb": "^6.18.0",
    "pg": "^8.16.3",
    "toad-scheduler": "^3.1.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "uuid": "^11.1.0"
  },
  "engines": {
    "node": "22.x"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,mjs}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{html,css}": [
      "prettier --write",
      "git add"
    ]
  }
}
